# Example .env file for Upwork API Client
# Copy this to .env and fill in your credentials

# Required: Your Upwork API credentials
# Get these from https://www.upwork.com/ab/account-security/api
UPWORK_CLIENT_ID=your_client_id_here
UPWORK_CLIENT_SECRET=your_client_secret_here
UPWORK_REDIRECT_URI=http://localhost:3000/callback

# Optional: If you already have tokens
UPWORK_ACCESS_TOKEN=
UPWORK_REFRESH_TOKEN=

# Optional: For enterprise features
UPWORK_ORGANIZATION_ID=
